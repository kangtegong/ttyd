#if LWS_LIBRARY_VERSION_NUMBER >= [m[31m4000000[m
[32mstatic[m [32mconst[m [32muint32_t[m backoff_ms[] = {[31m1000[m, [31m2000[m, [31m3000[m, [31m4000[m, [31m5000[m};
[32mstatic[m lws_retry_bo_t retry = {
    .retry_ms_table = backoff_ms,
    .retry_ms_table_count = LWS_ARRAY_SIZE(backoff_ms),
    .conceal_count = LWS_ARRAY_SIZE(backoff_ms),
    .secs_since_valid_ping = [31m5[m,
    .secs_since_valid_hangup = [31m10[m,
    .jitter_percent = [31m0[m,
